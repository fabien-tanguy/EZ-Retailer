<ul data-role="listview" data-theme="a" data-inset="true">
	<li><%= link_to "Ajouter une photo", new_admin_photo_path, "data-ajax" => false %></li>
</ul> 
<ul data-role="listview" data-theme="c" data-inset="true"  data-divider-theme ="a">
	<li data-role="list-divider">
		Mon album
	</li>
	<% if @photos %> 
		<% @photos.each do |photo| %>					
			<li><%= link_to(image_tag(photo.image_url,:width => "80", :height => "80")+"<h1>Visualiser</h1>".html_safe,admin_photo_path(photo))  %></li>
		<% end %>
	<% end %>	
</ul>


